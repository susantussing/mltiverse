{
  "extends": [
    "react-app",
    "prettier",
    "prettier/react",
    "airbnb",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": ["error", {"allow": ["_id"]}],
    "react/prop-types": 0,
    "no-console": ["error", {"allow": ["warn", "error"]}]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}